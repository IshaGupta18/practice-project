<h1> User Sign Up </h1>
<%= form_for @user do |f| %>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<br>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<br>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :age %>
	<%= f.number_field :age %>
	<br>
	<%= f.submit %>
<% end %>
